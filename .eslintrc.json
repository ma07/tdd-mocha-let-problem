{
 "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "mocha": true,
    "node": false

  },
  "parser": "babel-eslint",
  "rules": {
    "global-require": 0,
    "no-use-before-define": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "no-unused-expressions": 0
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "globals": {
    "expect": true,
    "$": true,
    "jQuery": true,
    "TOKEN": true,
    "SERVICE_URL": true,
    "module": true,
    "require": true
  }
}
